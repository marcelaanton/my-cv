//- the model, the data
-
  var header ='#what I can do';
  var whatCanIDoList =[
    {
      title:'Starter Front End Web Developer',
      skillList:[
        'HTML',
        'CSS',
        ''
      ]
    },
        {
      title:'Starter Back End Web Developer',
      skillList:[
        'Java',
        'MySQL',
        ''
      ]
    }

  ];

  //- the presentation, the view
#what-can-i-do
.what-can-i-do
  .what-can-i-do__header=header
  each whatCanIDo in whatCanIDoList
    .what-can-i-do__title=whatCanIDo.title
    .what-can-i-do__list
      each skill in whatCanIDo.skillList
        .what-can-i-do__skill=skill